<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.keleon.myapplication.MainActivity">

    <ImageView
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:id="@+id/imageView"
        android:background="@drawable/imageview"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/button4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Take Picture"
        android:id="@+id/button"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:background="#5a6c7f"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log"
        android:id="@+id/button2"
        android:background="#5a6c7f"
        android:textColor="#ffffff"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save/Upload"
        android:id="@+id/button3"
        android:background="#5a6c7f"
        android:textColor="#ffffff"
        android:layout_alignTop="@+id/button2"
        android:layout_alignParentEnd="true" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText"
        android:text="Origin Shipper Cert"
        android:inputType="text"
        android:autoText="false"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true"
        android:background="#eb8157"
        android:textColor="#ffffff" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText2"
        android:text="Harvest Date"
        android:layout_below="@+id/editText"
        android:layout_alignParentStart="true"
        android:background="#eb8157"
        android:textColor="#ffffff"
        android:layout_alignEnd="@+id/editText" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText3"
        android:text="Harvest Location"
        android:layout_below="@+id/editText2"
        android:layout_alignParentStart="true"
        android:background="#eb8157"
        android:textColor="#ffffff"
        android:layout_alignEnd="@+id/editText2" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText4"
        android:text="Type of Shellfish"
        android:layout_below="@+id/editText3"
        android:layout_alignParentStart="true"
        android:background="#eb8157"
        android:textColor="#ffffff"
        android:layout_alignEnd="@+id/editText3" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText5"
        android:layout_below="@+id/editText4"
        android:layout_alignStart="@+id/editText2"
        android:text="Quantity"
        android:background="#eb8157"
        android:textColor="#ffffff"
        android:layout_toStartOf="@+id/button"
        android:layout_alignEnd="@+id/editText4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText6"
        android:text="Temperature"
        android:background="#10cfbd"
        android:layout_alignParentEnd="true"
        android:layout_above="@+id/editText7"
        android:layout_toEndOf="@+id/button"
        android:layout_alignTop="@+id/editText" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText7"
        android:layout_alignParentEnd="true"
        android:text="GEO Location"
        android:background="#10cfbd"
        android:layout_above="@+id/editText3"
        android:layout_toEndOf="@+id/button" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText8"
        android:layout_below="@+id/editText7"
        android:layout_alignParentEnd="true"
        android:text="Time of Scan"
        android:background="#10cfbd"
        android:layout_alignBottom="@+id/editText3"
        android:layout_alignStart="@+id/editText7" />

    <EditText
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText9"
        android:layout_alignParentEnd="true"
        android:text="Date of Scan"
        android:background="#10cfbd"
        android:layout_above="@+id/editText5"
        android:layout_toEndOf="@+id/button" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText10"
        android:text="Unique ID"
        android:background="#10cfbd"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/editText5"
        android:layout_alignStart="@+id/editText9"
        android:layout_alignTop="@+id/editText5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tag"
        android:id="@+id/button4"
        android:background="#eb8157"
        android:textColor="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temp"
        android:id="@+id/button5"
        android:layout_above="@+id/imageView"
        android:layout_toEndOf="@+id/button4"
        android:background="#10cfbd"/>

</RelativeLayout>
